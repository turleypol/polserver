Function CreateEinzel(who, byref ret, byref tiles, byref color) // {{{
  Var z, itemdef:={}, iTarget, item, items:={}, itemserials:={};

  If (!tiles[ret.keys[2]])
    SendSysMessage(who, "No item selected");
