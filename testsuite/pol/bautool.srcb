                EndIf
              EndIf
            Else //centerpiece
              itemdef:=ChooseRandom(tiles);
              If (!itemdef)
                SendSysMessage(who, "Middle piece is missing.");
              Else
                itemdef:=SplitWords(itemdef,",");
                item:=CreateItemAtLocation(x,y,tar1.z+CInt(itemdef[2])+z,CInt(itemdef[1]),1,who.realm);
                If (item)
                  item.color:=color;
                  item.movable:=0;
