use uo;
use os;

include ":control:internal/icp-util";
include ":control:internal/cmdbar";

program Logon( who )

  if ( !ICP_LoadUserSetting( who, "NoCmdBarLogin" ) && !who.dead && who.cmdlevel )
    Start_Script( ":control:commands/coun/cmdbar", who );
  endif

  return 1;
endprogram
