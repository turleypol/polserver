use uo;
use os;

include ":timedScripts:timedScripts";

program TimedScript_EndParalysis( defender )
  var ts_cfg_elem := TS_GetCfgElem( "Paralysis" );
  var buff_icon := CInt( ts_cfg_elem.BuffIcon );

  defender.paralyzed := 0;
  defender.delbuff( buff_icon );

  return 1;
endprogram
