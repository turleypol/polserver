use uo;
use os;

include ":attributes:attributes";
include ":damage:damage";

program TimedScript_endFireResistBoost( params )
  var mobile := params[1];
  var points := CInt( GetObjProperty( mobile, "FireResistBoost" ) );
  points := -1 * points;
  ModifyResistance( mobile, DMG_FIRE, points );
  EraseObjProperty( mobile, "FireResistBoost" );

  return 1;
endprogram
