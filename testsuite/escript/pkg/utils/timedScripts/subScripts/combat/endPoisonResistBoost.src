use uo;
use os;

include ":attributes:attributes";
include ":damage:damage";

program TimedScript_endPoisonResistBoost( params )
  var mobile := params[1];
  var points := CInt( GetObjProperty( mobile, "PoisonResistBoost" ) );
  points := -1 * points;
  ModifyResistance( mobile, DMG_POISON, points );
  EraseObjProperty( mobile, "PoisonResistBoost" );

  return 1;
endprogram
