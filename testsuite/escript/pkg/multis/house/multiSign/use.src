program Mulit_House_sign( who, sign )
  var hserial := GetObjProperty( sign, "houseserial" );
  if ( !hserial )
    return;
  endif
  var house := SystemFindObjectBySerial( CInt( hserial ) );
  if ( !house )
    SendSysMessage( who, "Cannot find this house in the world data.",
                    color := 33 );
    return 0;
  endif
  var bank_region := CP_DEFAULT_AREA;
  var oserial := GetObjProperty( sign, "ownerserial" );

  SetPlaqueName( sign, house );
  if ( GetObjProperty( sign, "Public" ) )
    data[31] := "Declare this house Private";
    data[30] := "Change the sign type";
    layout.append( "page 3" );
    layout.append( "button 160 190 2714 2715 0 4 26" );
  else
    data[31] := "Declare this house Public";
    data[30] := "Change the master key";
    layout.append( "page 3" );
    layout.append( "button 160 190 2714 2715 1 4 16" );
  endif
  if ( who.serial == oserial )
    data[6] := who.name;
    SetObjProperty( sign, "lastownername", who.name );
  else
    data[6] := GetObjProperty( sign, "lastownername" );
  endif
  data[8] := GetObjProperty( sign, "RemainingLockdowns" );
  data[10] := GetObjProperty( sign, "RemainingSecures" );
  var friend_ := 0, cown := 0;
  var friendlist := sign.GetFriendList();
  var coown := sign.GetCoOwnerList();
  if ( who.serial in friendlist )
    friend_ := 1;
  elseif ( who.serial in coown )
    cown := 1;
  endif
  var result;
  var acct := GetObjProperty( sign, "owneracct" );
  if ( ( cown == 1 ) || ( friend_ == 1 ) || ( who.acctname == acct ) ||
       ( who.cmdlevel > 2 ) )
    result := SendDialogGump( who, layout, data );
  else
    SendDialogGump( who, nonlayout, data );
  endif
  if ( ( oserial == who.serial ) || ( cown ) )
    SetObjProperty( sign, "decay", ( ReadGameClock() + 2592000 ) ); // Refreshed for 30 days.
    SendSysMessageCL( who, 1043293, color := 66 ); // Your house's age and contents have been refreshed.
  endif
  // Friends can see house lists.
  if ( sign.IsAFriend( who ) )
    case ( result[0] )
      2:  ListCoowner( sign, who ); // list co-owners
      5:  ListFriend( sign, who ); // list friends
      11: ListBan( sign, who ); // list bans
    endcase
  endif
  if ( ( acct == who.acctname ) || ( cown ) || ( who.cmdlevel > 2 ) )
    if ( result[0] > 2979 && result[0] < 3085 )
      ChangeSign( who, house, sign, result[0] );
    endif
    case ( result[0] )
      1:
        SendSysMessage( who, "Enter a name for this house.(nothing to exit)",
                        color := 88 );
        var newname := RequestInput( who, sign, "" );
        if ( !newname )
          SendSysMessageCL( who, 1042023, color := 33 ); // Cancelled.
        elseif ( ( len( newname ) > 20 ) )
          SendSysMessage( who, "House sign names may not be longer than 20 characters.",
                          color := 33 );
        elseif ( len( SplitWords( newname ) ) > 5 )
          SendSysMessage( who, "House sign names are limited to 5 words or less.",
                          color := 33 );
        else
          SetName( sign, newname );
          SetObjProperty( sign, "signname", newname );
        endif
      2:  ListCoowner( sign, who ); // list co-owners
      3:  AddCoowner( sign, who ); // add co-owner
      4:  DeleteCoowner( sign, who ); // delete co-owner
      5:  ListFriend( sign, who ); // list friends
      6:  AddFriend( sign, who ); // add friend
      7:  DeleteFriend( sign, who ); // delete friend
      8:
        if ( confirmationSizable( who, "Clear List?" ) ) // clear friend list
          sign.WipeFriendsList();
          SendSysMessage( who, "Friend list has been cleared.", color := 66 );
        else
          SendSysMessageCL( who, 1042023, color := 33 ); // Cancelled.
        endif
      9:  AddBan( sign, who ); // add a ban
      10: Eject( sign, who ); // eject someone
      11: ListBan( sign, who ); // list bans
      12: DeleteBan( sign, who ); // remove a ban
      13:
        if ( ( oserial == who.serial ) or ( who.cmdlevel > 2 ) ) // transfer ownership
          var targetow := Target( who );
          SendSysMessage( who, "Transfer the house to whom?" );
          if ( ChangeHouseOwner( who, targetow, sign, bank_region ) == 1 )
            SendSysMessage( who, "House transfer was successful.", color := 66 );
          else
            SendSysMessage( who, "House transfer was NOT successful.",
                            color := 33 );
          endif
        endif
      14:
        if ( oserial == who.serial ) // demolish house
          if ( SmallYesNo( who, "are you sure?" ) )
            demolish( house, sign, who, bank_region );
          else
            SendSysMessageCL( who, 1042023, color := 33 ); // Cancelled.
          endif
          return;
        endif
      16:
        if ( oserial == who.serial ) // change locks
          ChangeLocks( who, house );
        endif
      17:
        if ( SmallYesNo( who, "Clear List?" ) ) // clear co-owner list
          EraseObjProperty( house, "coownlist" );
          SendSysMessage( who, "Co-owner list has been cleared." );
        else
          SendSysMessageCL( who, 1042023, color := 33 ); // Cancelled.
        endif
      19:
        if ( oserial == who.serial ) // make private
          DeclarePrivPub( who, house, sign );
        endif
    endcase
  endif
endprogram

