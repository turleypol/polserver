function AddFriend( hsign, who )
  // var friendlist := hsign.GetFriendList();
  SendSysMessageCL( who, 501317, color := 88 ); // Target the person you wish to name a friend of your household.
  var targetchr := Target( who );
  if ( !targetchr )
    SendSysMessageCL( who, 1042023, color := 33 ); // Cancelled.
    return;
  endif
  if ( !targetchr.isA( POLCLASS_MOBILE ) )
    SendSysMessage( who, "That cannot be added to the list.", color := 33 );
    return;
  endif
  var result := hsign.AddFriend( targetchr );
  if ( !result )
    SendSysMessage( who, "That person is already a friend.", color := 33 );
    return;
  elseif ( result < 0 )
    SendSysMessage( who, "Your friend list is full.", color := 33 );
    return;
  elseif ( targetchr.acctname == GetObjProperty( hsign, "owneracct" ) )
    SendSysMessageCL( who, 1042023, color := 33 ); // Cancelled.
    return;
  endif
  SendSysMessage( who, targetchr.name + " was friended to your house",
                  color := 66 );
  SendSysMessageCL( targetchr, 501337, color := 88 ); // You have been made a friend of this house.
endfunction

function DeleteFriend( hsign, who )
  var z := 0;
  var fillname;
  var retval1 := 0;
  var friendlist := hsign.GetFriendList();
  var newlist := {};
  foreach thing in friendlist
    fillname := SystemFindObjectBySerial( thing );
    if ( !fillname )
      fillname := SystemFindObjectBySerial( thing, SYSFIND_SEARCH_OFFLINE_MOBILES );
    endif
    if ( fillname )
      newlist.append( thing );
      datafriendremove[z + 2] := fillname.name;
      z := z + 1;
    endif
  endforeach
  hsign.SetFriendList( newlist );
  retval1 := SendDialogGump( who, layoutfriendremove, datafriendremove );
  var friendcheck, returncheck;
  if ( retval1[0] > 2 && retval1[0] <= 22 )
    returncheck := retval1[0] - 2;
    friendcheck := friendlist[returncheck];
    var unfriended := SystemFindObjectBySerial( friendcheck );
    var result := hsign.RemoveFriend( unfriended );
    if ( result > 0 )
      SendSysMessageCL( who, 501298, color := 66 ); // Friend removed from list.
    elseif ( !result )
      SendSysMessage( who, "The friendlist is empty.", color := 33 );
    elseif ( result == -1 )
      SendSysMessage( who, "There is no person in that slot.", color := 33 );
    endif
  endif
endfunction

