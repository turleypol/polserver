function AddBan( hsign, who )
  // var banlist := hsign.GetBanList();
  SendSysMessageCL( who, 502014, color := 88 ); // Select the victim.
  var targetchr := Target( who );
  if ( !targetchr )
    SendSysMessageCL( who, 1042023, color := 33 ); // Cancelled.
    return;
  endif
  if ( !targetchr.isA( POLCLASS_MOBILE ) )
    SendSysMessage( who, "That cannot be added to the list.", color := 33 );
    return;
  endif
  var result := hsign.IsBanned( targetchr );
  if ( result )
    SendSysMessageCL( who, 501356, color := 42 ); // This person is already banned!
    return;
  endif
  if ( targetchr.multi.serial == GetObjProperty( hsign, "houseserial" ) )
    MoveObjectToLocation( targetchr, hsign.x, hsign.y, hsign.z, hsign.realm );
