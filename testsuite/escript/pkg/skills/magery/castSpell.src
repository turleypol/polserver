use uo;
use os;
use cfgfile;

program castSpell( who, spell_id )

  var script := Start_Script( "spellStarter", { who, spell_id } );
  if ( script.errortext )
    SendSysMessage( who, "Error - Could not start spell starter ->"
                  + script.errortext );
    return 0;
  endif

  return 1;
endprogram
