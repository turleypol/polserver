use uo;
use os;

program canRemove( mobile, container, unused item, move_type )
  if ( mobile.cmdlevel )
    return 1;
  elseif ( move_type == MOVETYPE_COREMOVE )
    return 1;
  else
    PrintTextAbovePrivate( container, "That cannot be removed.", mobile );
    return 0;
  endif
endprogram
