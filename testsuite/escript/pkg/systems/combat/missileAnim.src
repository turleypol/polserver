use uo;
use os;

program missile_anim( params )

  var att_info := params[1];
  var def_info := params[2];
  Sleepms( 800 );
  PlayMovingEffect( att_info.mobile, def_info.mobile,
                    GetObjProperty( att_info.mobile.weapon, "ProjectileAnim" ),
                    10, 0 );

endprogram
